{
    "name": "mindmap",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build",
        "preview": "vite preview",
        "test": "vitest",
        "coverage": "vitest run --coverage",
        "prepare": "husky",
        "lint": "eslint .",
