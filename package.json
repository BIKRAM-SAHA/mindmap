        "react-redux": "^9.2.0",
        "react-responsive": "^10.0.1",
        "react-toastify": "^11.0.5",
        "uuid": "^11.1.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.19.0",
        "@types/node": "^22.13.1",
        "@types/react": "^19.0.8",
        "@types/react-dom": "^19.0.3",
        "@vitejs/plugin-react-swc": "^3.5.0",
        "eslint": "^9.19.0",
        "eslint-plugin-react-hooks": "^5.0.0",
        "eslint-plugin-react-refresh": "^0.4.18",
        "globals": "^15.14.0",
        "husky": "^9.1.7",
        "jsdom": "^26.0.0",
        "lint-staged": "^16.2.7",
        "prettier": "3.8.0",
